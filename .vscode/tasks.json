{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "group": "build",
      "type": "shell",
      "command": "dotnet",
      "args": [
        "msbuild",
        "C:/Game/project/FPS.sln",
        "/restore",
        "/t:Build",
        "/p:Configuration=Debug",
        "/v:normal",
        "/p:GodotTargetPlatform=windows",
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}